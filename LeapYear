#Description --> Return true if entered year is leap year


def is_leap(year):
    leap = False

    # Write your logic here
    if year % 4 ==0  and year % 400!=0:
        print.is_leap ("True")
    if year % 100 == 0 and year % 400 ==0:
        print.is_leap("True")
    return leap

year = int(input())
